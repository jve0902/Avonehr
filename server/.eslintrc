{
  "env": {
      "es6": true,
      "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "camelcase": 0,
    "consistent-return": ["warn", { "treatUndefinedAsUnspecified": false }],
    "prefer-destructuring": ["error", {"object": true, "array": false}]
  }
}